@model FeedAreaEditNoteViewModel
<div id="target">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h2 class="modal-title" id="myModalLabel">Feed Area Credit</h2>
    </div>
    <div class="modal-body">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <div>
                <span>@Model.Message</span>
            </div>
        </div>
        <br />
    </div>
    <div class="modal-footer">
        <button class="btn" type="button" data-dismiss="modal">Cancel</button>
        @*<a asp-page="/Range/RanchFields/Index">Continue</a>*@
        <input class="btn btn-primary" type="submit" value="Continue" onclick="window.location.href='/Ranch/RanchFields/Index'" />
    </div>
</div>