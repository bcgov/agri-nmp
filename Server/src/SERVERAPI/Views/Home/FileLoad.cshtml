@model SERVERAPI.ViewModels.FileLoadViewModel

@{
    Layout = null;
}
<div id="target">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">File Load</h4>
    </div>

    <form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="FileLoad">
        <div class="modal-body">
            <div class="row">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <div class="col-md-10">
                        <br />
                        <p>Please select a file to upload:</p>
                        <br />
                        <input class="form-control-file" type="file" name="files" id="FileUpload1" accept=".nmp" />
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button class="btn" type="button" data-dismiss="modal">Cancel</button>
            <input class="btn btn-primary" type="submit" value="Upload" />
        </div>
    </form>
</div>
